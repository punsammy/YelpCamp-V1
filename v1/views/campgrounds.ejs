<% include partials/header %>

<h1>This is the Campground Page!</h1>

<a href="/" class="ui secondary button">Go Back</a>
<a href="/campgrounds/new" class="ui secondary button" id="rightside-button">Add One</a>


<% campgrounds.forEach(function(campground) { %>
  <div class="campground-info">
    <h4><%= campground.name  %></h4>
    <img src="<%= campground.image %>">
  </div>
<% }); %>

